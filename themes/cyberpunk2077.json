{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Cyberpunk 2077 (Neon Noir)",
  "author": "Jason Sandeman",
  "themes": [
    {
      "name": "Cyberpunk 2077 (Neon Noir)",
      "appearance": "dark",
      "style": {
        "background": "#0D0F14",
        "surface.background": "#1E2028",
        "elevated_surface.background": "#2A2D38",

        "border": "#2E2F35",
        "border.variant": "#37EBF310",
        "border.focused": "#E455AE",
        "border.selected": "#E455AE",
        "border.transparent": "#00000000",
        "border.disabled": "#2E2F3580",

        "pane_group.border": "#37EBF3",

        "element.background": "#37EBF320",
        "element.hover": "#37EBF350",
        "element.active": "#37EBF380",
        "element.selected": "#37EBF390",
        "element.disabled": "#37EBF310",
        "drop_target.background": "#37EBF360",

        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#37EBF325",
        "ghost_element.active": "#37EBF345",
        "ghost_element.selected": "#37EBF365",
        "ghost_element.disabled": "#37EBF310",

        "text": "#F8F8F2",
        "text.muted": "#F8F8F280",
        "text.placeholder": "#F8F8F260",
        "text.disabled": "#F8F8F230",
        "text.accent": "#37EBF3",

        "icon": "#F8F8F2",
        "icon.muted": "#F8F8F260",
        "icon.disabled": "#F8F8F230",
        "icon.placeholder": "#F8F8F240",
        "icon.accent": "#37EBF3",

        "title_bar.background": "#0D0F14",
        "status_bar.background": "#0D0F14",
        "toolbar.background": "#1E2028",
        "tab_bar.background": "#1E2028",

        "tab.inactive_background": "#1E2028",
        "tab.active_background": "#37EBF320",

        "search.match_background": "#E455AE30",

        "panel.background": "#0D0F14",
        "panel.focused_border": "#E455AE",
        "pane.focused_border": "#E455AE",

        "scrollbar.thumb.background": "#37EBF3bb",
        "scrollbar.thumb.hover_background": "#37EBF3ee",
        "scrollbar.thumb.border": "#00000000",
        "scrollbar.track.background": "#1E202800",
        "scrollbar.track.border": "#00000000",

        "editor.background": "#1E2028",
        "editor.foreground": "#FDF500",
        "editor.gutter.background": "#1E2028",
        "editor.subheader.background": "#1E2028",
        "editor.active_line.background": "#1E202850",
        "editor.highlighted_line.background": "#37EBF320",
        "editor.line_number": "#9370DB",
        "editor.active_line_number": "#37EBF3",
        "editor.invisible": "#FDF50040",
        "editor.wrap_guide": "#9370DB",
        "editor.active_wrap_guide": "#9370DB",
        "editor.document_highlight.read_background": "#37EBF330",
        "editor.document_highlight.write_background": "#37EBF36b",

        "cursor": "#CB1DCD",

        "terminal.background": "#1E2028",
        "terminal.foreground": "#FDF500",
        "terminal.bright_foreground": "#37EBF3",
        "terminal.dim_foreground": "#FDF50060",
        "terminal.ansi.black": "#1E2028",
        "terminal.ansi.bright_black": "#37EBF3",
        "terminal.ansi.red": "#EDAF97",
        "terminal.ansi.bright_red": "#EDAF97",
        "terminal.ansi.green": "#9370DB",
        "terminal.ansi.bright_green": "#2CF6B3",
        "terminal.ansi.yellow": "#CB1DCD",
        "terminal.ansi.bright_yellow": "#CB1DCD",
        "terminal.ansi.blue": "#37EBF3",
        "terminal.ansi.bright_blue": "#37EBF3",
        "terminal.ansi.magenta": "#CB1DCD",
        "terminal.ansi.bright_magenta": "#CB1DCD",
        "terminal.ansi.cyan": "#E455AE",
        "terminal.ansi.bright_cyan": "#E455AE",
        "terminal.ansi.white": "#37EBF3",
        "terminal.ansi.bright_white": "#F8F8F2",

        "link_text.hover": "#37EBF3",

        "conflict": "#CB1DCD",
        "created": "#2CF6B3",
        "deleted": "#FF5555",
        "error": "#FF5555",
        "hidden": "#37EBF360",
        "hint": "#37EBF370",
        "ignored": "#6272A4",
        "info": "#37EBF3",
        "modified": "#20BF55",
        "predictive": "#37EBF360",
        "renamed": "#0E79B2",
        "success": "#2CF6B3",
        "unreachable": "#6272A4",
        "warning": "#CB1DCD",

        "players": [
          { "cursor": "#CB1DCD", "selection": "#CB1DCD30" },
          { "cursor": "#2CF6B3", "selection": "#2CF6B330" },
          { "cursor": "#37EBF3", "selection": "#37EBF330" },
          { "cursor": "#9370DB", "selection": "#9370DB30" }
        ],

        "syntax": {
          "attribute": { "color": "#37EBF3", "font_style": "italic" },
          "boolean": { "color": "#E55934" },
          "comment": { "color": "#6272A4" },
          "constant": { "color": "#37EBF3" },
          "constructor": { "color": "#2CF6B3" },
          "emphasis": { "font_style": "italic" },
          "emphasis.strong": { "font_weight": 700 },
          "function": { "color": "#2CF6B3" },
          "keyword": { "color": "#CB1DCD", "font_weight": 700 },
          "number": { "color": "#E55934" },
          "operator": { "color": "#CB1DCD" },
          "parameter": { "color": "#9370DB", "font_style": "italic" },
          "property": { "color": "#37EBF390" },
          "punctuation": { "color": "#0E79B2" },
          "string": { "color": "#FDF500" },
          "string.escape": { "color": "#0E79B2" },
          "tag": { "color": "#0E79B2" },
          "type": { "color": "#2CF6B3" },
          "variable": { "color": "#37EBF3" },
          "variable.special": { "color": "#37EBF3", "font_style": "italic" }
        }
      }
    }
  ]
}
